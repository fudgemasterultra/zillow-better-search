{
  "name": "Zillow Better Search",
  "description": "An application for making better searches",
  "version": "1.0",
  "manifest_version": 2,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": ["activeTab", "webRequest", "declarativeNetRequest"],
  "content_scripts": [
    {
      "matches": ["https://zillow.com/*"],
      "js": ["./static/js/content.js"]
    }
  ]
}
